{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "parser": "babel-eslint",
    "rules": {
      "strict": 0,
      "react/prop-types": 0,
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "react/jsx-filename-extension": [0, { "extensions": [".js", ".jsx"] }]
    },
    "globals": {
      "document": true,
      "window": true
    }
}
